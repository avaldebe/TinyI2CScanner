void setup() {
  OzOled.init();
  OzOled.clearDisplay();      // clear screen, start position to top left corner
  OzOled.setNormalDisplay();  // normal mode (i.e non-inverse mode)
  OzOled.setPageMode();       // addressing mode to Page Mode
  wellcome("I2C Scanner","7bit addresses");
  delay(1500);
  OzOled.clearDisplay();      // clear screen, start position to top left corner
  OzOled.drawBitmap(frame, 0, 0, 16, 8);
}

void loop() {

}

void wellcome(const char *title, const char *subtitle) {
  const byte x_mid = OzOLED_Max_X/16;
  const byte y_mid = OzOLED_Max_Y/16-1;

  byte mlen = 0;
  while(title[mlen]) mlen++;
  if(mlen>0) OzOled.printString(title, x_mid-mlen/2, y_mid-1);
  mlen = 0;
  while(subtitle[mlen]) mlen++;
  if(mlen>0) OzOled.printString(subtitle, x_mid-mlen/2, y_mid+1);
}
